  "scripts": {
    "build": "yarn docker:start && yarn build:clean && yarn lint && yarn jwt:create && yarn build:assets && yarn build:swagger && tsc && webpack-cli src/frontend/js/index.js --config webpack-config.js",
    "build:ci": "yarn build:clean && yarn lint && yarn jwt:create && yarn build:assets && yarn build:swagger && tsc && webpack-cli ./src/frontend/js/index.js --config webpack-config.js",
    "build:assets": "copyfiles -u 1 'src/**/*.json' 'src/**/*.yml' 'src/**/*.html' 'src/assets/images/*.jpg' 'src/assets/images/*.jpeg' 'src/assets/images/*.png' 'src/assets/images/*.ico' build",
    "build:clean": "yarn clean:build",
    "build:swagger": "node ./src/tasks/swagger.js",
    "clean:build": "del ./build ./spec.json ./package/payload",
    "clean": "yarn docker:clean && yarn clean:build && yarn test:clean",
    "debug": "yarn build && node --inspect-brk=57645 ./build/app/app.js",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down && del ./.mariadb",
    "jwt:create": "node ./src/tasks/jwt-create.js",
    "lint": "tslint src/**/*.ts",
    "lint-fix": "tslint --fix src/**/*.ts",
    "start": "yarn docker:start && nodemon",
    "restart": "yarn docker:clean && yarn clean:build && yarn test:clean && yarn docker:stop && yarn docker:start && nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:clean": "del .nyc_output coverage"
  },
