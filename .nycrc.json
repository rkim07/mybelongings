{
    "all": true,
    "reportDir": "coverage",
    "checkCoverage": false,
    "lines": 0,
    "functions": 0,
    "statements": 80,
    "branches": 0,
    "cache": true,
    "instrument": true,
    "sourceMap": true,
    "extension": [
        ".ts"
    ],
    "require": [
        "ts-node/register",
        "source-map-support/register",
        "reflect-metadata"
    ],
    "exclude": [
        "**/node_modules/**",
        "build/test/**",
        "build/tasks/**",
        "build/gulp/**"
    ],
    "include": [
        "build/**/*.js"
    ]
}
